def registrar():
    print("=== Registro de Datos del Trabajador ===")
    
    nombre = input("Nombre: ")
    nacimiento = input("Fecha de nacimiento (dd/mm/aaaa): ")
    contratacion = input("Tipo de contratación: ")
    sexo = input("Sexo: ")
    estudio = input("Último grado de estudio: ")
    cedula = input("Cédula profesional: ")
    domicilio = input("Domicilio: ")
    telefono = input("Teléfono: ")
    correo = input("Correo electrónico: ")
    ingreso = input("Fecha de ingreso (dd/mm/aaaa): ")

    print("\n=== Datos del registro ===")
    print("Nombre:", nombre)
    print("Fecha de nacimiento:", nacimiento)
    print("Tipo de contratación:", contratacion)
    print("Sexo:", sexo)
    print("Último grado de estudio:", estudio)
    print("Cédula profesional:", cedula)
    print("Domicilio:", domicilio)
    print("Teléfono:", telefono)
    print("Correo electrónico:", correo)
    print("Fecha de ingreso:", ingreso)

registrar()
